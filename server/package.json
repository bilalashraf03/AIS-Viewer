{
  "name": "ais-viewer-backend",
  "version": "1.0.0",
  "description": "AIS Viewer Backend - Real-time vessel tracking API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:old": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:aisstream": "tsx src/scripts/test-aisstream.ts",
    "test:batch-params": "tsx src/scripts/test-batch-params.ts"
  },
  "keywords": [
    "ais",
    "vessel",
    "tracking",
    "maritime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
